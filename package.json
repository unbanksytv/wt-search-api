{
  "name": "wt-search-api",
  "version": "0.0.0",
  "description": "NodeJS app that enables quick search over data from Winding Tree platform",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "WT_CONFIG=test ./node_modules/.bin/nyc --reporter=text ./node_modules/mocha/bin/mocha --recursive --timeout 20000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "node src/index.js",
    "dev": "WT_CONFIG=dev node src/index.js",
    "createdb-dev": "WT_CONFIG=dev node management/createdb.js",
    "createdb": "node management/createdb.js",
    "_deploy-playground": "now -e NODE_ENV=production --team windingtree --token $NOW_TOKEN --docker --public --local-config=now-playground.json",
    "_alias-playground": "now alias --team windingtree --token=$NOW_TOKEN --local-config=now-playground.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windingtree/wt-search-api.git"
  },
  "keywords": [
    "winding-tree",
    "api",
    "nodejs",
    "javascript"
  ],
  "author": "WT Dev Team <support@winidngtree.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/windingtree/wt-search-api/issues"
  },
  "homepage": "https://github.com/windingtree/wt-search-api#readme",
  "dependencies": {
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "morgan": "^1.9.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sqlite3": "^4.0.2",
    "web3": "^1.0.0-beta.36",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.1.0",
    "nyc": "^13.0.1",
    "sinon": "^6.1.4",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "nyc": {
    "exclude": [
      "test/",
      "src/config"
    ]
  }
}
